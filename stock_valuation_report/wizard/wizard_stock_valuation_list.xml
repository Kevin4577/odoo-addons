<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_report_stock_list" model="ir.ui.view">
            <field name="name">Stock Valuation List</field>
            <field name="model">wizard.stock.valuation.list</field>
            <field name="arch" type="xml">
                <form string="Confirm">
                    <group>
                        <field name="location_id"/>  
                    </group>        
                    <field name="stock_valuation_lines">
                        <tree>
                            <field name='default_code'/>
                            <field name='product_name'/>
                            <field name='uom_name'/>
                            <field name='inventory_quantity_before_start_date'/>
                            <field name='incoming_inventory_quantity'/>
                            <field name='outgoing_inventory_quantity'/>
                            <field name='inventory_quantity_before_end_date'/>
                            <field name='safety_stock_quantity'/>
                            <field name='below_safety_stock'/>
                            <field name='location_name'/>
                        </tree>
                    </field>
                    <footer>
                        <button name="xlsx_export"
                                type="object"
                                string="Export"
                                class="oe_highlight"/>
                    </footer>
               </form>
            </field>
        </record>
    </data>
</odoo>
