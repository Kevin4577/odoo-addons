<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_stock_valuation_user" model="res.groups">
        <field name="name">Stock Valuation User</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_stock_valuation_manager" model="res.groups">
        <field name="name">Stock Valuation Manager</field>
        <field name="implied_ids"
               eval="[(4, ref('stock_valuation_report.group_stock_valuation_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
</odoo>
