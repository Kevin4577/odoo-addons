<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="trading_purchase_requisition_report_py3o" model="ir.actions.report.xml">
        <field name="name">Requisition</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">stock.picking</field>
        <field name="report_name">trading_purchase_requisition_report</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">ods</field>
        <field name="module">trading_purchase_requisition_report</field>
        <field eval="True" name="py3o_multi_in_one"/>
        <field name="py3o_template_fallback">report/outbound_order_template.ods</field>
    </record>

    <record id="trading_purchase_requisition_report_py3o_print_action" model="ir.values">
        <field eval="'action'" name="key" />
        <field eval="'client_print_multi'" name="key2" />
        <field name="model">stock.picking</field>
        <field name="name">Requisition</field>
        <field eval="'ir.actions.report.xml,' + str(trading_purchase_requisition_report_py3o)"
               name="value"/>
    </record>

</odoo>
