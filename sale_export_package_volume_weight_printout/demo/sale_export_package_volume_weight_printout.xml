<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="local_py3o_server" model="py3o.server">
        <field name="url">http://localhost:8765/form</field>
    </record>

    <record id="sale_export_package_volume_weight_printout_py3o" model="ir.actions.report.xml">
        <field name="name">Sale Export Package Volume Weight Printout</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">stock.picking</field>
        <field name="report_name">package_volume_weight</field>
        <field name="report_type">py3o</field>
        <field name="py3o_filetype">ods</field>
        <field name="py3o_server_id" ref="local_py3o_server"/>
        <field name="module">sale_export_package_volume_weight_printout</field>
        <field name="py3o_template_fallback">demo/stock_pack_list_report_template.ods</field>
    </record>

    <record id="sale_export_package_volume_weight_printout_py3o_print_action" model="ir.values">
        <field eval="'action'" name="key" />
        <field eval="'client_print_multi'" name="key2" />
        <field name="model">stock.picking</field>
        <field name="name">Sale Export Package Volume Weight Printout</field>
        <field eval="'ir.actions.report.xml,'+str(sale_export_package_volume_weight_printout_py3o)" name="value"/>
    </record>

</odoo>
